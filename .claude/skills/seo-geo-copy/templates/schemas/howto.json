{
  "@context": "https://schema.org",
  "@type": "HowTo",
  "name": "{{HOWTO_TITLE}}",
  "description": "{{HOWTO_DESCRIPTION}}",
  "image": {
    "@type": "ImageObject",
    "url": "{{FEATURED_IMAGE_URL}}",
    "width": 1200,
    "height": 630
  },
  "totalTime": "PT{{TOTAL_MINUTES}}M",
  "estimatedCost": {
    "@type": "MonetaryAmount",
    "currency": "{{CURRENCY_CODE}}",
    "value": "{{COST_VALUE}}"
  },
  "supply": [
    {
      "@type": "HowToSupply",
      "name": "{{SUPPLY_1}}"
    },
    {
      "@type": "HowToSupply",
      "name": "{{SUPPLY_2}}"
    }
  ],
  "tool": [
    {
      "@type": "HowToTool",
      "name": "{{TOOL_1}}"
    },
    {
      "@type": "HowToTool",
      "name": "{{TOOL_2}}"
    }
  ],
  "step": [
    {
      "@type": "HowToStep",
      "name": "{{STEP_1_NAME}}",
      "text": "{{STEP_1_DESCRIPTION}}",
      "url": "{{PAGE_URL}}#step1",
      "image": "{{STEP_1_IMAGE_URL}}"
    },
    {
      "@type": "HowToStep",
      "name": "{{STEP_2_NAME}}",
      "text": "{{STEP_2_DESCRIPTION}}",
      "url": "{{PAGE_URL}}#step2",
      "image": "{{STEP_2_IMAGE_URL}}"
    },
    {
      "@type": "HowToStep",
      "name": "{{STEP_3_NAME}}",
      "text": "{{STEP_3_DESCRIPTION}}",
      "url": "{{PAGE_URL}}#step3",
      "image": "{{STEP_3_IMAGE_URL}}"
    }
  ]
}
